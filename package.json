{
    "name": "appily-agent",
    "version": "1.0.0",
    "description": "A TypeScript coding agent powered by Claude 3.7 that generates optimized Next.js projects for static hosting",
    "main": "dist/index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "start:no-realtime": "ts-node src/index.ts --no-realtime",
        "dev": "ts-node-dev --respawn src/index.ts",
        "dev:no-realtime": "ts-node-dev --respawn src/index.ts --no-realtime",
        "build": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "claude",
        "ai",
        "coding",
        "nextjs",
        "web",
        "development",
        "static-hosting",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@ai-sdk/anthropic": "^1.2.2",
        "@ai-sdk/deepseek": "^0.2.2",
        "@ai-sdk/google": "^1.2.3",
        "@ai-sdk/groq": "^1.2.1",
        "@ai-sdk/openai": "^1.3.4",
        "@aws-sdk/client-s3": "^3.777.0",
        "@aws-sdk/lib-storage": "^3.777.0",
        "@openrouter/ai-sdk-provider": "^0.4.5",
        "@types/cors": "^2.8.17",
        "ai": "^4.2.8",
        "axios": "^1.6.7",
        "body-parser": "^1.20.2",
        "chalk": "^4.1.2",
        "chokidar": "^4.0.3",
        "cors": "^2.8.5",
        "diff": "^7.0.0",
        "dotenv": "^16.4.1",
        "execa": "^5.1.1",
        "express": "^4.18.2",
        "firebase-admin": "^11.11.1",
        "fs-extra": "^11.3.0",
        "log-update": "^6.1.0",
        "openai": "^4.91.1",
        "ora": "^8.2.0",
        "strip-ansi": "^7.1.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@types/body-parser": "^1.19.5",
        "@types/diff": "^7.0.2",
        "@types/express": "^4.17.21",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.10.6",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "eslint": "^9.23.0",
        "globals": "^16.0.0",
        "nodemon": "^3.0.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
